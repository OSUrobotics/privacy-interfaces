<?xml version="1.0"?>
<package>
  <name>ceiling_localizer</name>
  <version>0.0.0</version>
  <description>This package provides a WIP localization system for the turtlebot through the use of an overhead camera.  This can be used in lieu of a system like AMCL.</description>

  <!-- One maintainer tag required, multiple allowed, one person per tag --> 
  <!-- Example:  -->
